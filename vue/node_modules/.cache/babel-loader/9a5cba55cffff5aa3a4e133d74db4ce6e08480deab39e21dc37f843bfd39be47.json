        }\n      });\n    },\n    // 新增初始化对话框\n    add() {\n      this.form = {};\n      this.dialogVisible = true;\n    },\n    // 编辑初始化模态框\n    edit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    save() {\n      if (!this.form.id) {\n        // 没有ID，说明是新增操作\n        request.post(baseUrl, this.form).then(res => {\n          if (res.code === '0') {\n            this.$notify.success(\"添加成功\");\n            this.dialogVisible = false;\n            this.load();\n          } else {\n            this.$notify.error(res.msg);\n          }\n        })\n      } else {\n        // 有ID，说明是更新操作\n        request.put(baseUrl, this.form).then(res => {\n          if (res.code === '0') {\n            this.$notify.success('更新成功');\n            this.dialogVisible = false;\n            this.load();\n          } else {\n            this.$notify.error(res.msg);\n          }\n        })\n      }\n    },\n    // 根据ID删除某条数据\n    del(id) {\n      request.delete(baseUrl + id).then(res => {\n        if (res.code === '0') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n\t\tjdtpSuccessUpload(res) {\n\t\t\tthis.form.img = res.data;\n\t\t},\n\t\t// 加载商品\n    loadGoods() {\n\t\t\trequest.get(\"/goods\").then(res => {\n\t\t\t\tif (res.code === '0') {\n\t\t\t\t\tthis.goodsData = res.data;\n\t\t\t\t} else {\n\t\t\t\t\tthis.$notify.error(res.msg);\n\t\t\t\t}\n\t\t\t})\n\t\t},\n\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n  }\n}\n</script>\n"],"mappings":"AA+DA,OAAAA,OAAA;AACA,OAAAC,UAAA;;AAEA;AACA,IAAAC,OAAA;AAEA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,QAAA;MACAC,SAAA;MACAC,OAAA;MACAC,aAAA;MACAC,MAAA;MACAC,IAAA;MACAC,KAAA;MACAC,SAAA;IAEA;EACA;EACA;EACAC,QAAA;IACA;IACA,KAAAC,SAAA;IACA,KAAAC,IAAA;EACA;EACA;EACAC,OAAA;IACAC,eAAA;MACA,IAAAC,KAAA;MACA,IAAAC,KAAA;MACAC,UAAA;QACA,IAAAC,IAAA,GAAAC,QA